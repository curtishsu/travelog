{
  "name": "passport",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "clean:next": "node scripts/clean-next.mjs",
    "dev": "NEXT_DIST_DIR=.next-dev npm run clean:next && NEXT_DIST_DIR=.next-dev next dev",
    "build": "NEXT_DIST_DIR=.next npm run clean:next && NEXT_DIST_DIR=.next next build && NEXT_DIST_DIR=.next node scripts/ensure-next-layout-css.mjs",
    "start": "NEXT_DIST_DIR=.next next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
